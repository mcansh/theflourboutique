{
  "name": "theflourboutique",
  "productName": "The Flour Boutique",
  "productShortName": "TFB",
  "version": "1.2.0-canary.2",
  "description": "The Flour Boutique is a bakery from Lynette McAnsh based in Utica, Michigan. Cookies range from various shapes and sizes as well as from basic to elaborate.",
  "private": true,
  "engines": {
    "node": ">= 7.6.0"
  },
  "scripts": {
    "start": "node -r @std/esm ./server.js",
    "dev": "next",
    "build": "next build",
    "dev:server": "nodemon --watch ./server.js -r @std/esm server",
    "test": "eslint ./",
    "deploy": "now --docker && now alias",
    "deploy:pre": "now --docker && now alias canary.theflourboutique.com"
  },
  "now": {
    "dotenv": ".env",
    "alias": [
      "theflourboutique.com",
      "www.theflourboutique.com"
    ]
  },
  "@std/esm": {
    "esm": "js",
    "cjs": true
  },
  "dependencies": {
    "@std/esm": "0.21.2",
    "apollo-cache-inmemory": "1.1.7",
    "apollo-client": "2.2.3",
    "apollo-link-http": "1.3.3",
    "babel-plugin-inline-dotenv": "1.1.2",
    "date-fns": "1.29.0",
    "graphql": "0.13.0",
    "graphql-tag": "2.7.3",
    "isomorphic-unfetch": "2.0.0",
    "jsonwebtoken": "8.1.1",
    "next": "5.0.0",
    "next-cookies": "1.0.2",
    "nprogress": "0.2.0",
    "polka": "0.3.3",
    "prop-types": "15.6.0",
    "raven-js": "3.22.2",
    "react": "16.2.0",
    "react-apollo": "2.0.4",
    "react-dom": "16.2.0",
    "react-ga": "2.4.1",
    "sw-precache-webpack-plugin": "0.11.4",
    "validator": "9.4.0",
    "webpack": "3.11.0"
  },
  "devDependencies": {
    "babel-eslint": "8.2.1",
    "eslint": "4.17.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.6.1",
    "nodemon": "1.14.12",
    "prettier": "1.10.2"
  }
}
