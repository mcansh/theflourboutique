{
  "name": "theflourboutique",
  "productName": "The Flour Boutique",
  "productShortName": "TFB",
  "version": "1.2.0-canary.4",
  "description": "The Flour Boutique is a bakery from Lynette McAnsh based in Utica, Michigan. Cookies range from various shapes and sizes as well as from basic to elaborate.",
  "private": true,
  "engines": {
    "node": ">= 7.6.0"
  },
  "scripts": {
    "start": "node -r @std/esm ./server.js",
    "dev": "next",
    "build": "next build && node ./scripts/default-lang",
    "dev:server": "nodemon --watch ./server.js -r @std/esm server",
    "test": "eslint ./",
    "deploy": "now --docker && now alias",
    "deploy:pre": "now --docker && now alias canary.theflourboutique.com"
  },
  "now": {
    "dotenv": ".env",
    "alias": [
      "theflourboutique.com",
      "www.theflourboutique.com"
    ]
  },
  "@std/esm": {
    "esm": "js",
    "cjs": true
  },
  "dependencies": {
    "@std/esm": "0.23.2",
    "accepts": "1.3.4",
    "apollo-boost": "0.1.1",
    "babel-plugin-inline-dotenv": "1.1.2",
    "babel-plugin-react-intl": "2.4.0",
    "babel-plugin-styled-components": "1.5.0",
    "date-fns": "1.29.0",
    "glob": "7.1.2",
    "graphql": "0.13.1",
    "intl": "1.2.5",
    "isomorphic-unfetch": "2.0.0",
    "jwt-decode": "2.2.0",
    "next": "5.0.0",
    "next-cookies": "1.0.2",
    "next-workbox-webpack-plugin": "1.0.1",
    "nprogress": "0.2.0",
    "polished": "1.9.2",
    "polka": "0.3.4",
    "prop-types": "15.6.1",
    "raven-js": "3.22.4",
    "react": "16.2.0",
    "react-apollo": "2.1.0-beta.3",
    "react-dom": "16.2.0",
    "react-ga": "2.4.1",
    "react-intl": "2.4.0",
    "styled-components": "3.1.6",
    "validator": "9.4.1",
    "webpack": "4.0.1"
  },
  "devDependencies": {
    "babel-eslint": "8.2.2",
    "eslint": "4.18.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-mcansh": "1.0.2",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.7.0",
    "nodemon": "1.17.1",
    "prettier": "1.11.0"
  }
}
